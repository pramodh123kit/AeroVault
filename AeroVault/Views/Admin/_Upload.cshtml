    <!-- Header Section -->
    <div class="header-container">
        <h2>Upload History</h2>

        <button class="upload-btn">
            <img src="/Content/Assets/Uploadbtn1.svg" class="upload-btn-icn" alt="upload Icon">
            Upload Files
        </button>
    </div>

    <!-- Filters Section -->
    <div class="filters-section">
        <div class="filters-container">
            <div class="dropdown-group">
                <div class="dropdown-wrapper">
                    <label>Department</label>
                    <select class="filter-dropdown" name="department">
                        <option value="all">All</option>
                        <option value="it">Information Technology</option>
                        <option value="hr">Human Resources</option>
                        <option value="gh">Ground Handling</option>
                        <option value="finance">Finance</option>
                        <option value="em">Engineering & Maintenance</option>
                        <option value="cargo">Cargo</option>
                    </select>
                </div>

                <div class="dropdown-wrapper">
                    <label>System</label>
                    <select class="filter-dropdown" name="system">
                        <option value="all">All</option>
                        <option value="amos">AMOS</option>
                        <option value="easypass">EasyPass</option>
                    </select>
                </div>

                <div class="dropdown-wrapper">
                    <label>Category</label>
                    <select class="filter-dropdown" name="category">
                        <option value="all">All</option>
                        <option value="technical">Technical</option>
                        <option value="training">Training</option>
                    </select>
                </div>
                <div class="file-search-container">
                    <img src="/Content/Assets/search-icon.svg" class="search-icon" alt="Search Icon" />
                    <input id="SystemfileSearch" onkeyup="filterFiles()" placeholder="Search" type="text"
                        class="file-search-input" />
                </div>
            </div>


