@model AeroVault.Models.OverviewViewModel

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div id="main-content">
    @if (Model == null)
    {
        <p>Error: Model is null.</p>
    }
    else
    {
        @Html.Partial("_Overview", Model) <!-- Pass the strongly typed model -->
    }
</div>

<link rel="stylesheet" href="/Content/css/UserSidebarStyles.css">
<script src="/Scripts/UserSidebarScripts.js"></script>